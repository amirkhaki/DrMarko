@{
	Layout = "_Aviato";
}
<section class="page-header">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="content">
					<h1 class="page-name">@ViewData["Title"]</h1>
					<ol class="breadcrumb">
						<li><a asp-controller="Home" asp-action="Index">Home</a></li>
						<li class="active">@ViewData["Pagination"]</li>
					</ol>
				</div>
			</div>
		</div>
	</div>
</section>
            @RenderBody()
